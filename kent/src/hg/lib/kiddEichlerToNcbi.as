table kiddEichlerToNcbi
"Map clone IDs from Kidd..Eichler '08 to NCBI clone and/or end read accessions"
    (
    string name;		"Clone ID used in Kidd..Eichler '08"
    string cloneAcc;		"NCBI clone accession"
    int endF;		"NCBI trace_name of forward end read"
    int endR;		"NCBI trace_name of reverse end read"
    )
