table hugo
"A cross-reference table between HUGO Gene Nomenclature Committee (HGNC) IDs and other database IDs."
	(
	string hgnc;		"HGNC number"
	string symbol;		"HGNC gene symbol"
	string name;		"HGNC gene name"
	string map;		"Gene map location"
	string mim;		"MIM ID"
	string pmid1;		"pmid 1"
	string pmid2;		"pmid 2"
	string refseq;		"RefSeq ID"
	string aliases;		"Aliases"
	string widthdraws;	"Withdrawn gene symbols"
	string locuslink;	"LocusLink ID"
	string gdbID;		"GDB ID"
	string swissprot;	"Swiss-Prot ID"
	)
